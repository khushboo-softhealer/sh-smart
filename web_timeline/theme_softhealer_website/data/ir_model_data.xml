<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sh_helpdesk.model_sh_helpdesk_ticket" model="ir.model">
            <field name="website_form_key">sh_create_ticket</field>
            <field name="website_form_default_field_id"
                ref="sh_helpdesk.field_sh_helpdesk_ticket__description" />
            <field name="website_form_access">True</field>
            <field name="website_form_label">Create an Ticket</field>
        </record>

        <function model="ir.model.fields" name="formbuilder_whitelist">
            <value>sh.helpdesk.ticket</value>
            <value
                eval="[
                'email',
                'description',
                'hardware_form_partner_name',
                'hardware_form_contact_no',
                'hardware_form_company',
                'hardware_form_country_id',
                'hardware_form_check_url',
                'ticket_type',
            ]" />
        </function>
    </data>
</odoo>