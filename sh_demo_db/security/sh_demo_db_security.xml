<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="sh_demo_database_access" model="ir.module.category">
			<field name="name">Softhealer Demo Database Module Access</field>
		</record>
		<record id="sh_group_demo_db_user" model="res.groups">
			<field name="name">User</field>
			<field name="category_id" ref="sh_demo_db.sh_demo_database_access"/>
			<field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
		</record>
		<record id="sh_group_demo_db_manager" model="res.groups">
			<field name="name">Manager</field>
			<field name="category_id" ref="sh_demo_db.sh_demo_database_access"/>
			<field name="implied_ids" eval="[(4,ref('sh_demo_db.sh_group_demo_db_user'))]"/>
			<field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
		</record>
	</data>
</odoo>