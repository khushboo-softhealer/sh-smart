<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sh_training_project_res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.inherit.view.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="project.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@name='project_time']" position="after">
                <h2>Training Management</h2>
                <div class="row mt16 o_settings_container" name="project_project">
                    <div class="col-12 col-lg-6 o_setting_box" name="project_project_management">
                        <div class="o_setting_left_pane"></div>
                        <div class="o_setting_right_pane">
                            <label for="sh_training_project_id" />
                            <field name="sh_training_project_id" />
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
    
</odoo>