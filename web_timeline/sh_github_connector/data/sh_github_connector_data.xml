<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Multi Action (Sync Repo) -->
    <record id="sh_sync_github" model="ir.actions.server" >
        <field name="name">Sync Repo(s)</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="model_sh_git_repo" />
        <field name="binding_model_id" ref="model_sh_git_repo" />
        <field name="state">code</field>
        <field name="code">
            action = records.sync_github()
        </field>
    </record>
    <!-- Multi Action (Sync Repo Branches) -->
    <record id="sh_import_branches" model="ir.actions.server" >
        <field name="name">Import Branch(s)</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="model_sh_git_repo" />
        <field name="binding_model_id" ref="model_sh_git_repo" />
        <field name="state">code</field>
        <field name="code">
            action = records.import_branch_action()
        </field>
    </record>
    <!-- Multi Action (Sync Full Index) -->
    <record id="sh_sync_full_index" model="ir.actions.server" >
        <field name="name">Sync Full Index</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="model_sh_index_queue" />
        <field name="binding_model_id" ref="model_sh_index_queue" />
        <field name="state">code</field>
        <field name="code">
            action = records.sync_full_index()
        </field>
    </record>
