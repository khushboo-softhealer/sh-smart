<?xml version="1.0" encoding="utf-8"?>
<odoo>



    <!-- Template  About Us for website basic view -->
    <template id="theme_softhealer_store_about_us_tmpl" name="Who We Are - Dedicated Odoo Professionals">
        <t t-call="website.layout">
   <!-- 
    
==================================
 About Us for Store
==================================

-->

<div class="oe_structure oe_empty" id="wrap">
   <!-- 
    
==================================
 About Us for Store
==================================

-->
    <!-- banner start -->
    <section id="sh_store_banner"
        class="sh_store_privacy_policy_banner sh_other_page_banner">
        <div class="container">
            <div class="sh_banner_content">
                <h1 class="sh_banner_title">About Us</h1>
                <ul class="sh_banner_breadcrumb">
                    <li>
                        <a class="sh_banner_breadcrumb_link" href="/">Home <span
                                class="sh_banner_breadcrumb_icon fa fa-angle-right"></span>
                        </a>
                    </li>
                    <li>
                        <span class="sh_banner_normal_text">About Us</span>
                    </li>
                </ul>
            </div>
        </div>
    </section>
    <!-- banner end -->
    <!-- store abouts us snippet start -->
    <section id="sh_store_abouts_us_snippet" class="sh_store_abouts_us_snippet">
        <div class="container">
            <div class="sh_store_abouts_us_main">
                <div class="sh_inner_box">
                    <div class="row align-items-center">
                        <div
                            class="col-xxl-6 col-xl-6 col-lg-6 col-md-12 col-sm-12 col-xs-12 col-12 align-self-center">
                            <div class="sh_column_inner_box"
                                >
                                <div class="sh_img_box">
                                    <img
                                        src="/theme_softhealer_store/static/src/img/about_us/about_store.webp"
                                        alt="About Softhealer" title="Who We Are - Dedicated Odoo Professionals" loading="lazy" />
                                </div>
                            </div>
                        </div>
                        <div
                            class="col-xxl-6 col-xl-6 col-lg-6 col-md-12 col-sm-12 col-xs-12 col-12">
                            <div class="sh_store_abouts_us_content"
                                >
                                <h5 class="sh_sub_title">About Us</h5>
                                <h2 class="sh_store_abouts_us_content_title">Crafting
                                    Digital Solutions to Empowering Your
                                    Business</h2>
                                <div class="sh_store_abouts_us_content_box">
                                    <div
                                        class="sh_store_abouts_us_content_text">We
                                        understand that every business is
                                        unique, with its
                                        own set of challenges and goals. That's
                                        why we take
                                        a personalized approach to crafting
                                        solutions that
                                        are tailored to your specific needs. We
                                        collaborate
                                        closely with our clients to gain a deep
                                        understanding of their business
                                        objectives, target
                                        audience, and industry landscape. This
                                        enables us to
                                        design digital solutions that not only
                                        meet your
                                        immediate needs but also position your
                                        business for
                                        long-term success.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- store abouts us snippet end -->
    <!-- store why choose us snippet start -->
    <section id="sh_store_why_choose_us_snippet"
        class="sh_store_why_choose_us_snippet">
        <div class="container">
            <div class="sh_store_why_choose_us_main">
                <div class="sh_store_why_choose_us_heading">
                    <h5 class="sh_heading_sub_title">Frames of Achievement</h5>
                    <h2 class="sh_store_why_choose_us_heading">Visualizing
                        Accomplishments
                        in a Captivating Manner</h2>
                </div>
                <div class="sh_store_why_choose_us_counter_main">
                    <div class="row">
                        <div
                            class="col-xxl-3 col-xl-3 col-lg-3 col-md-6 col-sm-12 col-xs-12 col-12">
                            <div class="sh_counter_box">
                                <div class="d-flex justify-content-center">
                                    <h2 class="sh_counter js_cls_counter_number">1200</h2><h2 class='sh_counter'>+</h2>
                                </div>
                                <p class="sh_counter_text">Total Apps</p>
                            </div>
                        </div>
                        <div
                            class="col-xxl-3 col-xl-3 col-lg-3 col-md-6 col-sm-12 col-xs-12 col-12">
                            <div class="sh_counter_box">
                                <div class="d-flex justify-content-center">
                                    <h2 class="sh_counter js_cls_counter_number">50</h2><h2 class='sh_counter'>+</h2>
                                </div>
                                <p class="sh_counter_text">Industrial Solutions</p>
                            </div>

                        </div>
                        <div
                            class="col-xxl-3 col-xl-3 col-lg-3 col-md-6 col-sm-12 col-xs-12 col-12">
                            <div class="sh_counter_box">
                                <div class="d-flex justify-content-center">
                                    <h2 class="sh_counter js_cls_counter_number">550</h2><h2 class='sh_counter'>+</h2>
                                </div>
                                <p class="sh_counter_text">Projects Completed</p>
                            </div>
                        </div>
                        <div
                            class="col-xxl-3 col-xl-3 col-lg-3 col-md-6 col-sm-12 col-xs-12 col-12">
                            <div class="sh_counter_box">
                                <div class="d-flex justify-content-center">
                                    <h2 class="sh_counter js_cls_counter_number">200</h2><h2 class='sh_counter'>+</h2>
                                </div>
                                <p class="sh_counter_text">ERP Implementation</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- store why choose us snippet end -->
    <!-- store our mission snippet start -->
    <section id="sh_store_our_mission_snippet"
        class="sh_store_our_mission_snippet">
        <div class="container">
            <div class="sh_store_our_mission_main">

                <div class="row align-items-center">
                    <div
                        class="col-xxl-6 col-xl-6 col-lg-6 col-md-12 col-sm-12 col-xs-12 col-12">
                        <div class="sh_store_our_mission_content"
                            >
                            <h5 class="sh_sub_title">Our Mission</h5>
                            <h2 class="sh_store_our_mission_content_title">Creating
                                a
                                Paperless Future for Businesses and Preserving
                                the
                                Environment</h2>
                            <div class="sh_store_our_mission_content_box">
                                <div
                                    class="sh_store_our_mission_content_text">At
                                    Custom
                                    Business Solutions, our mission is clear: to
                                    drive the
                                    transition towards a paperless business
                                    landscape while
                                    promoting environmental sustainability. We
                                    are committed
                                    to helping businesses embrace digital
                                    solutions that not
                                    only streamline their operations but also
                                    reduce their
                                    ecological footprint.
                                </div>
                            </div>
                        </div>
                    </div>
                    <div
                        class="col-xxl-6 col-xl-6 col-lg-6 col-md-12 col-sm-12 col-xs-12 col-12 align-self-center">
                        <div class="sh_column_inner_box"
                            >
                            <div class="sh_img_box">
                                <img
                                    src="/theme_softhealer_store/static/src/img/about_us/mission_store.webp"
                                    alt="Digital Solution" title="Who We Are - Dedicated Odoo Professionals" loading="lazy" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- store our mission us snippet end -->
</div>


        </t>
    </template>		
	

    <data noupdate="1">

        <!-- Default pagese  below 2 record must put in no update 1-->
        <record id="theme_softhealer_store_about_us_page" model="website.page">
            <field name="is_published">True</field>
            <field name="url">/about-us</field>
            <field name="view_id" ref="theme_softhealer_store.theme_softhealer_store_about_us_tmpl"/>
            <field name="track">True</field>
            <field name="website_id" model="website" eval="obj().env['website'].sudo().get_current_website()"/>
        </record>

        <record id="theme_softhealer_store_about_us_menu" model="website.menu">
            <field name="name">About Us</field>
            <field name="url">/about-us</field>
            <field name="page_id" ref="theme_softhealer_store.theme_softhealer_store_about_us_page"/>
            <field name="sequence" type="int">10</field>
            <field name="website_id" model="website" eval="obj().env['website'].sudo().get_current_website()"/>
            <!-- <field name="parent_id" model="website.menu" eval="obj().env['website'].sudo().get_current_website().menu_id"/> -->
        </record>

    </data>





</odoo>
