<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- =====================
        Website Blog page
    ========================== -->
    <template id='theme_softhealer_store_blog_top_banner' name='Explore Our Odoo Blog for Latest Updates and Expert Advice'
        inherit_id='website_blog.blog_post_short'>

        <!--Replace
        blog banner -->
        <xpath expr='//div[@id="o_wblog_blog_top"]' position='replace'>
            <div id='o_wblog_blog_top'>
                <section id="sh_store_blog_banner" class="sh_store_blog_page_banner o_colored_level"
                    style="background-image: none;">
                    <div class="container">
                        <div class="sh_banner_content">
                            <h1 class="sh_banner_title">Blogs</h1>

                            <ul class="sh_banner_breadcrumb">
                                <li>
                                    <a class="sh_banner_breadcrumb_link" href="/">Home <span
                                            class="sh_banner_breadcrumb_icon fa fa-angle-right"></span>
                                    </a>
                                </li>
                                <li>
                                    <span class="sh_banner_normal_text">Blogs</span>
                                </li>
                            </ul>
                        </div>
                    </div>
                </section>
            </div>
        </xpath>

    </template>

    <template id="sh_store_blogs_nav" inherit_id="website_blog.blogs_nav">
        <!-- <div class="container" position="attributes">
            <attribute name="class" separator=" " add="sh_cls_blogs_nav"></attribute>
        </div> -->
        <xpath expr="//div[hasclass('container')]" position="attributes">
            <attribute name="class" add="sh_cls_blogs_nav" separator=" "/>
        </xpath>
    </template>

    <template id="theme_softhealer_store_blog_head" name="custom blog head"
        inherit_id="website_blog.post_heading">
        <xpath expr="//div[hasclass('text-muted')]" position="after">
            <div t-if="len(blog_post.tag_ids)"
                class="o_wblog_post_short_tag_section d-flex align-items-center flex-wrap pt-2">
                <t t-foreach="blog_post.tag_ids" t-as="one_tag">
                    <a t-attf-href="#{blog_url(tag=tags_list(active_tag_ids, one_tag.id))}"
                        t-attf-class="badge mb-2 me-1 text-truncate #{one_tag.id in active_tag_ids and 'bg-primary text-light' or 'border text-primary'} post_link"
                        t-att-rel="len(active_tag_ids) and 'nofollow'"
                        t-esc="one_tag.name" />
                </t>
            </div>
        </xpath>

        <xpath expr="//a" position="attributes">
            <attribute name="t-att-title" separator=" " add="blog_post.name" remove="" value=""></attribute>
        </xpath>
    </template>


    <template id="theme_softhealer_store_blog_teaser" name="custom blog teaser"
        inherit_id="website_blog.post_teaser">
        <xpath expr="//div[hasclass('o_wblog_post_short_tag_section')]" position="replace"></xpath>
    </template>

    <template id="theme_softhealer_store_blog_info" name="custom blog info"
        inherit_id="website_blog.post_info">
        <xpath expr="//time" position="replace">
            <span class='px-2'>
                <i class="fa fa-calendar" />
                <time t-field="blog_post.post_date" class="text-nowrap fw-bold "
                    t-options="{'widget': 'datetime', 'date_only': 'true', 'format': 'medium'}" />
            </span>
        </xpath>
    </template>

    <!-- =====================
        Website Blog Post page
    ========================== -->

    <template id="theme_softhealer_store_blog_post_page" name="custom blog post page"
        inherit_id="website_blog.blog_post_complete">
        <xpath expr="//section[@id='o_wblog_post_top']" position="replace">
            <section id="sh_store_blog_page_banner"
                class="sh_store_blog_page_detail_banner o_colored_level"
                style="background-image: none;">
                <div class="container">
                    <div class="sh_banner_content">
                        <div class="d-flex justify-content-between align-items-center">
                            <h2 class="sh_banner_title">Blog Post</h2>
                          </div>

                        <ul class="sh_banner_breadcrumb">
                            <li>
                                <a class="sh_banner_breadcrumb_link" href="/">Home <span
                                        class="sh_banner_breadcrumb_icon fa fa-angle-right"></span>
                                </a>
                            </li>
                            <li>
                                <a class="sh_banner_breadcrumb_link" href="/blog">Blogs <span
                                        class="sh_banner_breadcrumb_icon fa fa-angle-right"></span>
                                </a>
                            </li>
                            <li>
                                <span class="sh_banner_normal_text" t-esc='blog_post.name'></span>
                            </li>
                        </ul>
