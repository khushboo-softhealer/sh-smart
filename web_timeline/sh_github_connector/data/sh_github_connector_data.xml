        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="model_product_product" />
        <field name="binding_model_id" ref="model_product_product" />
        <field name="state">code</field>
        <field name="code">
            action = records.mul_action_unlink_mul_blogs()
        </field>
    </record>
    <!-- Multi Action (Add Product(Module) In Queue) -->
    <record id="sh_product_module_in_queue" model="ir.actions.server" >
        <field name="name">Module In Queue</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="model_product_product" />
        <field name="binding_model_id" ref="model_product_product" />
        <field name="state">code</field>
        <field name="code">
            action = records.mul_action_add_module_in_queue()
        </field>
    </record>
    <!-- Multi Action (Get New Products) -->
    <record id="sh_action_get_new_products" model="ir.actions.server" >
