<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sh_type_of_command_view_tree" model="ir.ui.view">
        <field name="name">sh.type.of.command.view.tree</field>
        <field name="model">sh.type.of.command</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="name" />
                 <field name="description" />
                <field name="active" invisible="1"/>
            </tree>
        </field>
    </record>    

    <record id="sh_type_of_command_action" model="ir.actions.act_window">
        <field name="name">Type of Command</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">sh.type.of.command</field>
        <field name="view_mode">tree</field>        
    </record>

</odoo>
