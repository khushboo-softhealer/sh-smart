<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="sh_demo_db_config_setting_view" model="ir.ui.view">
		<field name="name">res.config.settings</field>
		<field name="model">res.config.settings</field>
		<field name="inherit_id" ref="sh_helpdesk.helpdesk_config_setting_view"/>
		<field name="arch" type="xml">
			<xpath expr="//div[@id='default_team_id']" position="after">
				<div class="row o_settings_container" id="demo_server_id">
                    <div class="col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="sh_expired_demo_db" />
                            <br />
                            <field name="sh_expired_demo_db" class="oe_inline" />
                        </div>
                        <div class="o_setting_left_pane"></div>
                    </div>
                    <div class="col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="sh_demo_db_user_ids" />
                            <br />
                            <field name="sh_demo_db_user_ids" class="oe_inline" widget="many2many_tags" />
                        </div>
                        <div class="o_setting_left_pane"></div>
                    </div>
					<div class="col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="sh_demo_type_id" />
                            <br />
                            <field name="sh_demo_type_id" class="oe_inline" />
                        </div>
                        <div class="o_setting_left_pane"></div>
                    </div>
					<div class="col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="sh_demo_stage_id" />
                            <br />
                            <field name="sh_demo_stage_id" class="oe_inline" />
                        </div>
                        <div class="o_setting_left_pane"></div>
                    </div>
                    <div class="col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="sh_demo_limit" />
                            <br />
                            <field name="sh_demo_limit" class="oe_inline" />
                        </div>
                        <div class="o_setting_left_pane"></div>
                    </div>
                    <div class="col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <label for="sh_db_drop_activity_assign_id" />
                            <br />
                            <field name="sh_db_drop_activity_assign_id" class="oe_inline" />
                        </div>
                        <div class="o_setting_left_pane"></div>
                    </div>
                </div>
			</xpath>
		</field>
	</record>
</odoo>