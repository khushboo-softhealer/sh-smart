    </template>

    <template id="theme_softhealer_theme_blog_post_page_content" name="custom blog page content"
        inherit_id="website_blog.blog_post_content">
        <xpath expr="//t[@t-call='website_blog.post_breadcrumbs']" position="replace"></xpath>

        <xpath expr="//div[hasclass('css_editable_mode_hidden')]" position="replace"></xpath>

    </template>


</odoo>