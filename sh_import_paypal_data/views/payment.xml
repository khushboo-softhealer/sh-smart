<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    
    <record id="account_payment_inherit_view_form" model="ir.ui.view">
        <field name="name">account.payment.inherit.view.form</field>
        <field name="model">account.payment</field>
        <field name="inherit_id" ref="account.view_account_payment_form"/>
        <field name="arch" type="xml">
            <field name = "payment_transaction_id" position = "after">
                <field name = "paypal_fees"/>
                <field name = "currency_of_fees"/>
                <field name = "paypal_bank_transfer_id"/>
            </field>
        </field>
    </record>
</odoo>