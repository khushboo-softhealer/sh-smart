<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <data noupdate="1">
	     <record model="ir.cron" id="sh_auto_fill_calendar">
	        <field name="name">Auto Fill Calendar</field>
	        <field name="active" eval="True"/>
	        <field name="interval_number">1</field>
	        <field name="interval_type">days</field>
	        <field name="numbercall">-1</field>
	        <field name="doall" eval="False"/>
	        <field name="model_id" ref="sh_task_time.model_resource_calendar_attendance"/>
	        <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 23:00:00')" />  
	        <field name="state">code</field>
	        <field name="code">model.auto_fill_calendar()</field>
	    </record>
    </data>

</odoo>
