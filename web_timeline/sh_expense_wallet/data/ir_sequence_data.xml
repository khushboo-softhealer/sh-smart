<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="walllet_sequence" model="ir.sequence">
        <field name="name">Wallet Sequence</field>
        <field name="code">wallet.entry</field>
        <field name="prefix">Wallet/%(year)s/</field>
        <field eval="1" name="number_next" />
        <field name="padding">5</field>        
        <field eval="False" name="company_id" />
    </record>

    <record id="employee_account_sequence" model="ir.sequence">
        <field name="name">Employee Account sequence</field>
        <field name="code">employee.account</field>
        <field name="prefix">EA</field>
        <field eval="1" name="number_next" />
        <field name="padding">5</field>        
        <field eval="False" name="company_id" />
    </record>

</odoo>