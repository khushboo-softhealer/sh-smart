                        <br />
                    </t>
                    <br />
                    <strong style="font-size: 12px;">Our odoo Appstore link for module : &amp;nbsp; </strong>
                    <span t-esc="sh.odoo_url" style="font-size: 12px;" />
                    <t t-if="sh.odoo_urls"> ,&amp;nbsp; <span style="font-size: 12px;" t-esc="sh.odoo_urls" />
                    <br />
                </t>
                <br />
                <strong style="font-size: 12px;">Open github repos : &amp;nbsp; </strong>
                <br />
                <span t-esc="sh.claim_data" style="font-size: 12px;" />
            </t>
            <t t-if="already_prod_id == sh.product_id.id">
                <br />
                <span t-esc="sh.claim_data" style="font-size: 12px;" />
            </t>
            <br />
            <t t-set="already_prod_id" t-value="sh.product_id.id" />
        </t>
        <br />
    </div>
</t>
</t>
</template>

<record id="paperformat_sh_claim_details" model="report.paperformat">
<field name="name">Copyright Claim Report</field>
<field name="default" eval="True" />
<field name="format">custom</field>
<field name="page_height">300</field>
<field name="page_width">300</field>
<field name="orientation">Portrait</field>
<field name="margin_top">1</field>
<field name="margin_bottom">0</field>
<field name="margin_left">5</field>
<field name="margin_right">5</field>
<field name="header_line" eval="False" />
<field name="header_spacing">5</field>
<field name="dpi">90</field>
</record>

<record id="sh_claim_details_report_action" model="ir.actions.report">
<field name="name">Copyright Claim Report</field>
<field name="model">sh.copyright.claim</field>
<field name="report_type">qweb-pdf</field>
<field name="report_name">sh_copyright_claim_project.sh_claim_details_report_doc</field>
<field name="report_file">sh_copyright_claim_project.sh_claim_details_report_doc</field>
<field name="print_report_name">'Copyright Claim Details Report'</field>
<field name="paperformat_id" ref="sh_copyright_claim_project.paperformat_sh_claim_details" />
</record>
</odoo>
