<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id='sh_import_failed_tree' model='ir.ui.view'>
        <field name="name">sh.import.failed.tree</field>
        <field name="model">sh.import.failed</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name" />
                <field name="field_type" />
                <field name="datetime" />
                <field name="error" />
                <field name="import_json" />
            </tree>
        </field>
    </record>

    <record id="sh_import_failed_action" model="ir.actions.act_window">
        <field name="name">Logger</field>
        <field name="res_model">sh.import.failed</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Logger
            </p>
        </field>
    </record>
</odoo>