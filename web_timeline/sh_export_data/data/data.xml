<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sh_export_sequence" model="ir.sequence">
			<field name="name">Export Sequence</field>
			<field name="code">sh.export</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">6</field>
        </record>
        
         <record model="ir.actions.server" id="sh_export_data_multi_action">
            <field name="name">Export Data</field>
            <field name="type">ir.actions.server</field>
            <field name="model_id" ref="model_sh_export_data"/>
	         <field name="binding_model_id" ref="model_sh_export_data"/>
            <field name="state">code</field>
            <field name="code">
                records.export_data_multi()
            </field>
        </record>

    </data>
</odoo>
