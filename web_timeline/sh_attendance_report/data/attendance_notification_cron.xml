<?xml version ="1.0" encoding="utf-8" ?>
<odoo>
	 <data>          
         <record id="sh_employee_work_hours_notify" model="ir.cron">
            <field name="name">Employee Work Hours Notify</field>
  			<field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
  			<field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 23:45:00')" />            
            <field name="model_id" ref="model_sh_employee_workhours_email" />
 			<field name="state">code</field>            
            <field name="code">model.notify_employee_workhours_fun()</field>
            <field name="priority" eval="5"/>
        </record>
     </data>   
</odoo>    