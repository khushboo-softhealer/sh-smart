<?xml version ="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="sh_project_form_view" model="ir.ui.view">
			<field name="name">sh.project.form.view</field>
			<field name="model">project.project</field>
			<field name="inherit_id" ref="project.edit_project"/>
			<field name="arch" type="xml">
				<field name="partner_id" position="after">
					<field name="sh_product_id"/>
				</field>			
			</field>
		</record>
		<record id="sh_timesheet_tree_view" model="ir.ui.view">
			<field name="name">sh.timesheet.tree.view</field>
			<field name="model">account.analytic.line</field>
			<field name="inherit_id" ref="hr_timesheet.hr_timesheet_line_tree"/>
			<field name="arch" type="xml">
				<field name="task_id" position="after">
					<field name="timesheet_invoice_id"/>
				</field>
			</field>
		</record>
	</data>
</odoo>