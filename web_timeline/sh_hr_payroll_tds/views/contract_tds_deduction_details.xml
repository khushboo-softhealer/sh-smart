<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sh_contract_tds_deduction_details_form" model="ir.ui.view">
        <field name="name">contract.tds.deduction.view.form</field>
        <field name="model">contract.tds.deduction</field>
        <field name="arch" type="xml">
            <form string="Deduction Details">
                    <group>
                        <field name="line_ids">
                            <tree editable="bottom">
                                <field name="name"/>
                                <field name="amount"/>
                            </tree>
                        </field>
                    </group>
            </form>
        </field>
    </record>

   
