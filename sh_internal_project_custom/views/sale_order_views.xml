<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sh_internal_project_sale_view_form" model="ir.ui.view">
        <field name="name">sh.sale.order.internal.form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form" />
        <field name="arch" type="xml">
            <xpath expr="//group[@name='partner_details']" position="inside">
                <field name="sh_move_task_to_preapp_store" />
            </xpath>
        </field>
    </record>

</odoo>
