<?xml version ="1.0" encoding="utf-8" ?>

<odoo>
	<data>
	
	<record id="sh_ir_cron_inherit_view_form" model="ir.ui.view">
			<field name="name">ir.cron.inherit.view.form</field>
			<field name="model">ir.cron</field>
			<field name="inherit_id" ref="base.ir_cron_view_form"/>
			<field name="arch" type="xml">
				<xpath expr="//sheet" position="after">
                    <!-- temp -->
					 <field name="res_model" invisbile="1"/>
                     <!-- ===== -->

					 <field name="res_model_id" invisbile="1"/>
					<div class="oe_chatter">
	                    <field name="message_follower_ids" widget="mail_followers"/>
	                    <field name="activity_ids" widget="mail_activity"/>
	                    <field name="message_ids" widget="mail_thread"/>
	                </div>
				</xpath>
				
			</field>
		</record>
	
	 <menuitem id="utm.menu_link_tracker_root" name="Link Tracker" 
	 groups="base.group_system" sequence="30" web_icon="utm,static/description/icon.png" />
	 
