<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_other_helpdesk">
        <field name="name">Helpdesk Management</field>
        <field name="sequence">60</field>
        <field name="visible" eval="0" />
    </record>
    <record id="sh_helpdesk.helpdesk_group_send_email" model="res.groups">
        <field name="name">Helpdesk Send Email</field>
        <field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk.helpdesk_group_estimation" model="res.groups">
        <field name="name">Helpdesk Estimation</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk.group_helpdesk_alarm" model="res.groups">
        <field name="name">Ticket Alarm</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk.group_helpdesk_sla_policy" model="res.groups">
        <field name="name">Helpdesk SLA Policy</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk.helpdesk_group_whatsapp" model="res.groups">
        <field name="name">Helpdesk Whatsapp Feature</field>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]" />
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk.sh_odoo_end_ticket_group" model="res.groups">
        <field name="name">Odoo End Tickets</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk.group_send_quick_reply" model="res.groups">
        <field name="name">Send Quick Reply</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_lead_qualification.group_lead_qualification" model="res.groups">
        <field name="name">Lead Qualification</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk_crm.sh_helpdesk_ticket_crm" model="res.groups">
        <field name="name">CRM Helpdesk Ticket</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk_so.sh_helpdesk_ticket_so" model="res.groups">
        <field name="name">Sale Helpdesk Ticket</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk_timesheet.group_helpdesk_timesheet" model="res.groups">
        <field name="name">Helpdesk Ticket Timesheet</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk_invoice.sh_helpdesk_ticket_invoice" model="res.groups">
        <field name="name">Invoice Helpdesk Ticket</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_odoo_order_reference_number_management.sh_odoo_order_reference_odoo_app_details" model="res.groups">
        <field name="name">Show Odoo Apps details tab in ticket</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk_task.group_helpdesk_task" model="res.groups">
        <field name="name">Helpdesk Task</field>
        <field name="category_id" ref="module_category_other_helpdesk"/>
    </record>
    <record id="sh_helpdesk_ticket_rule_portal" model="ir.rule">
        <field name="name">Portal Personal Tickets</field>
        <field name="model_id" ref="sh_helpdesk.model_sh_helpdesk_ticket"/>
        <field name="domain_force">[('message_partner_ids','child_of',[user.commercial_partner_id.id])]</field>
        <field name="groups" eval="[(4, ref('base.group_portal'))]"/>
        <field name="perm_unlink" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_create" eval="False"/>
    </record>
</odoo>